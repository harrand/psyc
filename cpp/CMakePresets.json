{
	"version": 4,
	"cmakeMinimumRequired":
	{
		"major": 3,
		"minor": 23,
		"patch": 0
	},
	"configurePresets":
	[
	{
		"name": "debug",
		"binaryDir": "${sourceDir}/build/debug",
		"cacheVariables":
		{
			"CMAKE_BUILD_TYPE":
			{
				"type": "string",
				"value": "Debug"
			}
		}
	},
	{
		"name": "release",
		"binaryDir": "${sourceDir}/build/release",
		"cacheVariables":
		{
			"CMAKE_BUILD_TYPE":
			{
				"type": "string",
				"value": "Release"
			}
		}
	}
	]
}
