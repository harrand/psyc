putchar :: (ch : i8 weak) -> u0 := extern;
print_digit :: (digit : i64) -> u0
{
	// ascii 48 == '0'
	putchar(48 + digit);
}

is_odd :: (val : i64) -> bool
{
	half : i64 := (val / 2);	
	low_half : i64 := ((val - 1) / 2);
	return (half == low_half);
}

main :: () -> i64
{
	print_digit(1);
	print_digit(2);
	print_digit(3);

	if is_odd(23456)
	{
		print_digit(8);
	}
	else
	{
		print_digit(9);
	}
	return 0;	
}

== default : build ==
{
	set_build_type(debug);
	set_linkage_type(executable);
}