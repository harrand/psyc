semal_program ::= struct
{
	root : ast&;
};

semal_find_build_metaregion ::= func(source : ast&) -> ast&
{
	return null;
};

semal_file ::= func(prog : semal_program mut&, source : ast&) -> v0
{
	build_metaregion ::= semal_find_build_metaregion(source);
	if(build_metaregion == null)
	{
		psyc_warning(source->loc, "File had no build-metaregion");
	}
};

== build ==
{
	add_source_file("ast.psy");
	add_source_file("diag.psy");
}
