irpos : struct
{
	func_cursor : u64;
	blk_cursor : u64;
};

irf_callconv : enum
{
	.sysv := 1;
	.win64 := 2;
};

// function
irf : struct
{
	params : irv mut?;
	params_count : u64;
	params_cap : u64;

	name : u8?;
	name_len : u64;
	conv : irf_callconv;
	regcur : u64;

	blks : irb mut?;
	blks_count : u64;
	blks_cap : u64;
};

// block
irb : struct
{
	inst : iri mut?;
	inst_count : u64;
	inst_cap : u64;
};
