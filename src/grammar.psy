grammar_install ::= func(-> v0)
{
	grammar_hashtable_insert(state1(
		token(lex_token.symbol),
		zero, zero),
	grammar_rule{.fn := token_symbol2symbol_expr;});
};

token_symbol2symbol_expr ::= func(nodes : ast mut?, nodes_count : u64 -> rule_result)
{
	return rule_result
	{
		.tag := rule.commit;
		.commit := zero;
	};
};
