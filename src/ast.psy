ast_expr_type ::= enum
{
	.unknown := 0;
	.symbol := 1;
};

ast_expr ::= struct
{
	type : ast_expr_type;
};

ast_stmt_type ::= enum
{
	.unknown := 0;
	.expr_stmt := 1;
};

ast_stmt ::= struct
{
	type : ast_stmt_type;
	expr : ast_expr;
};

ast_type ::= enum
{
	.unknown := 0;
	.statement := 1;
};

ast ::= struct
{
	type : ast_type;
	stmt : ast_stmt;
};
