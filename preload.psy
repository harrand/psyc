embed_data ::= struct
{
	data : u8?;
	size : u64;
};

srcloc ::= struct
{
	file : u8?;
	line : u64;
	column : u64;
};

_atomic_ordering ::= enum
{
	.monotonic := 2;
	.acquire := 3;
	.release := 4;
	.acqrel := 5;
	.seqcst := 6;
};

_atomic_op ::= enum
{
	.xchg := 0;
	.add := 1;
	.sub := 2;
	.and := 3;
	.nand := 4;
	.or := 5;
	.xor := 6;
	.smax := 7;
	.smin := 8;
	.umax := 9;
	.umin := 10;
};
